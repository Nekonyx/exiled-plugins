{
  "typescript.format.semicolons": "remove",
  "typescript.format.enable": false,
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Licenser
  "licenser.author": "Nekonyx",
  "licenser.license": "AGPLv3",
  "licenser.projectName": "EXILED Plugins",
  "licenser.disableAutoHeaderInsertion": false,
  "licenser.useSingleLineStyle": false,
  // TypeScript Import Sorter
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimit",
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 90,
  "importSorter.importStringConfiguration.hasSemicolon": false,
  "importSorter.importStringConfiguration.tabSize": 2,
  "importSorter.importStringConfiguration.quoteMark": "single",
  // Prettier
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.endOfLine": "lf",
  "prettier.useTabs": false,
  "prettier.packageManager": "yarn",
  "prettier.trailingComma": "none",
  "prettier.printWidth": 90
}
